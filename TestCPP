#include <iostream>

using namespace std;


//the length of the stack
int size();
int pop();
void push(int object);

//t is the last index that has an element of the array thats being used 
int t = 0;
int S[10];

int main(int argc, char** argv)
{

}

//returns the length of the stack
int size()
{
	//has +1 because if T is the index of the last element in the array, the array starts at 0, not 1
	return t + 1;
}

//removes and returns the top element
int pop()
{
	if (empty())
	{
		throw new StackEmptyException();
	}
	else
	{
		t = t - 1;
		return S[t + 1];
	}
}

//put the given object at the top of the stack
void push(int object)
{
	if (t = S.size() - 1)
	{
		throw new StackFullException();
	}
	else
	{
		t = t + 1;
		S[t] = object;
	}
}
